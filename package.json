    "keybindings": [
      {
        "command": "mdToolbar.fmt.bold",
        "key": "ctrl+b",
        "mac": "cmd+b",
        "when": "editorTextFocus && editorLangId == markdown"
      },
      {
        "command": "mdToolbar.fmt.italic",
        "key": "ctrl+i",
        "mac": "cmd+i",
        "when": "editorTextFocus && editorLangId == markdown"
      },
      {
        "command": "mdToolbar.code.inline",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c",
        "when": "editorTextFocus && editorLangId == markdown"
      },
      {
        "command": "mdToolbar.link.insert",
        "key": "ctrl+k",
        "mac": "cmd+k",
